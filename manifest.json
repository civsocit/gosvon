{
  "manifest_version": 2,
  "name": "GosVon Chrome extension",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["*://vk.com/*"],
      "js": ["jquery.min.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "ico.png"
  },
  "permissions": [
    "*://api.gosvon.net/*"
  ]
}